{
  "name": "ipa-chart-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next dist out node_modules/.cache",
    "vercel:deploy": "vercel --prod --force",
    "firebase:build": "FIREBASE_CI=true npm run build",
    "firebase:serve": "firebase serve",
    "firebase:deploy": "firebase deploy",
    "firebase:emulators": "firebase emulators:start",
    "pages:clean": "rm -rf .next dist out",
    "pages:build": "npm run pages:clean && GITHUB_ACTIONS=true GITHUB_REPOSITORY=nghicv/ipa-chart-web NODE_ENV=production npm run build",
    "pages:serve": "npx serve dist"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.3.5"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "@eslint/eslintrc": "^3",
    "firebase-tools": "^13.0.0",
    "serve": "^14.2.1"
  }
}
